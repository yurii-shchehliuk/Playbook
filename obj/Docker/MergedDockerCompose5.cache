name: dockercompose6365684555379158982
services:
  api.scrapping:
    build:
      context: C:\Users\hero1\source\repos\playbook1\Playbook
      dockerfile: API.Scrapping/Dockerfile
    container_name: api.scrapping
    depends_on:
      scrappingdb:
        condition: service_started
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      PlaybookDatabase:ConnectionString: mongodb://scrappingdb:27017
    image: apiscrapping
    networks:
      default: null
    ports:
    - mode: ingress
      target: 80
      published: "8000"
      protocol: tcp
  mgclient:
    container_name: mgclient
    image: mongoclient/mongoclient
    networks:
      default: null
    ports:
    - mode: ingress
      target: 3000
      published: "3000"
      protocol: tcp
  scrappingdb:
    container_name: scrappingdb
    image: mongo
    networks:
      default: null
    ports:
    - mode: ingress
      target: 27017
      published: "27017"
      protocol: tcp
    restart: always
    volumes:
    - type: volume
      source: mongodata
      target: /data/db
      volume: {}
networks:
  default:
    name: dockercompose6365684555379158982_default
volumes:
  mongodata:
    name: dockercompose6365684555379158982_mongodata